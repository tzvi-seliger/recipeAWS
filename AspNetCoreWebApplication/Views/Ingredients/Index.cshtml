@model List<AspNetCoreWebApplication.Models.Ingredient>;
@{
    ViewData["Title"] = "Index";
}

<ul>
    @{
        for (int i = 0; i < Model.Count; i++)
        {
            <li>
                <div>@Model[i].IngredientName</div>
            </li>
        }
    }
</ul>

<a href="~/ingredients/ingredientForm">Submit New Ingredient</a>
<a href="~/recipes">See All Recipes</a>

<h2>Index</h2>

